{
  "browserSync":{
    "port":[
      3000
    ]
  },
  "build": {
    "dir":[
      "./build"
    ],
    "vendor": {
      "dir": [
        "./build/assets/vendor"
      ],
      "files":[
        "./build/assets/vendor/**/*"
      ]
    },
    "js": {
      "dir": [
        "./build/assets/js"
      ],
      "files":[
        "./build/assets/js/**/*.js"
      ]
    },
    "css": {
      "dir": [
        "./build/assets/css"
      ],
      "files":[
        "./build/assets/css/**/*.css"
      ]
    },
    "images": {
      "dir": [
        "./build/assets/images"
      ],
      "files":[
        "./build/assets/images/**/*"
      ]
    },
    "html": {
      "files":[
        "./build/**/*.html"
      ]
    }
  },
  "src": {
    "vendor": {
      "dir": [
        "./src/template/assets/vendor"
      ],
      "files":[
        "./src/template/assets/vendor/**/*"
      ]
    },
    "js": {
      "dir": [
        "./src/template/assets/js"
      ],
      "files":[
        "./src/template/assets/js/**/*.js"
      ]
    },
    "scss": {
      "dir": [
        "./src/template/assets/scss"
      ],
      "files":[
        "./src/template/assets/scss/**/*.scss"
      ]
    },
    "images": {
      "dir": [
        "./src/template/assets/images"
      ],
      "files":[
        "./src/template/assets/images/**/*"
      ]
    },
    "html": {
      "files":[
        "./src/template/**/*.html"
      ]
    },
    "txtFiles":[
      "./src/template/*.txt",
      "./src/template/.*"
    ]
  }

}